{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/history",
      "dest": "/history/index.html"
    },
    {
      "src": "/History",
      "dest": "/history/index.html"
    },
    {
      "src": "/add",
      "dest": "/add/index.html"
    },
    {
      "src": "/Add",
      "dest": "/add/index.html"
    }
  ]
}
